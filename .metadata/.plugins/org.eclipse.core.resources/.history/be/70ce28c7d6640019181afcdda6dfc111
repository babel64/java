package first;

public class var_ch {
	
	public static void main(String[] arges) {
     //   공간 만둘기 = 자료형 변수명; 변수선언
	   int kor; // 정수형 값을 저장할 수 있는  kor이라는 변수
	   
	   string str; // 문자열을 저잘할 수 있는 str변수
	   
	    kor = 99; 123; // 변수 할당
	   
	   kor = kor + 8;
	   
	   kor = kor - 50;
	   
	   str="안녕하세요";
	   
	   System.out.println(kor);
   }
	  
}
